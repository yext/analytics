{
  "name": "@yext/answers-analytics",
  "version": "0.0.0",
  "description": "An analytics library for Yext Answers",
  "author": "slapshot@yext.com",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "typescript": "^4.1.5"
  },
  "jest": {
    "bail": 0,
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*.ts"
    ]
  }
}
